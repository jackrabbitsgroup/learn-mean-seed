{
	"all": {
		"modules":["_all"]
	},
	"allNoBuild": {
		"modules":["_all"],
		"comment":"'_all' is a special/reserved word for getting ALL modules",
		"skipModules": ["build", "buildTemp", "buildTempCss", "test"]
	},
	"allNoBuildCss": {
		"modules":["_all"],
		"comment":"like 'allNoBuild' except we WANT 'buildTempCss', which has compiled 'base.css' (from '_base.less') in it",
		"skipModules": ["build", "buildTemp", "test"]
	},
	"allMinified": {
		"modules":["bower.minified", "lib.minified", "buildTemp", "test"]
	},
	"nonMinified": {
		"modules":["_all"],
		"skipModules":["build", "buildTemp", "buildTempCss", "bower.minified", "lib.minified", "test"]
	},
	"nonMinifiedLint": {
		"modules":["_all"],
		"skipModules":["build", "buildTemp", "buildTempCss", "bower.minified", "lib.minified", "bower.nonMinified", "lib.nonMinified", "test"]
	},
	"testsProtractor": {
		"modules":["_all"],
		"skipModules": ["build", "buildTemp", "buildTempCss"]
	},
	"testsProtractorNoLessons": {
		"modules":["test"]
	},
	"testsUnitNoLessons": {
		"modules":["_all"],
		"skipModules": ["build", "buildTemp", "test",
			"modules.pages.angular.directive.use.forminput.angular-forminput-basic",
			"modules.pages.angular.controller.test.scope.angular-controller-test-scope"
		]
	},
	"testsLessons": {
		"comment": "Karma Unit tests will error if no tests so make sure there's at least one! (passwordReset is to ensure there is at least one)",
		"xModulesFullList":[
			"modules.pages.passwordReset",
			"modules.pages.angular.directive.use.forminput.angular-forminput-basic",
			"modules.pages.angular.controller.test.scope.angular-controller-test-scope"
		],
		"modules":[
			"modules.pages.angular.directive.use.forminput.angular-forminput-basic",
			"modules.pages.angular.controller.test.scope.angular-controller-test-scope"
		]
	}
}